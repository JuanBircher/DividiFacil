{
  "name": "dividi-facil.front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:stats": "ng build --stats-json --named-chunks",
    "analyze": "ng build --stats-json --named-chunks && npx webpack-bundle-analyzer dist/dividi-facil.front-end/stats.json",
    "analyze:prod": "ng build --configuration production --stats-json --named-chunks && npx webpack-bundle-analyzer dist/dividi-facil.front-end/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.0.6",
    "@angular/cdk": "20.0.5",
    "@angular/common": "20.0.6",
    "@angular/compiler": "20.0.6",
    "@angular/core": "20.0.6",
    "@angular/forms": "20.0.6",
    "@angular/material": "20.0.5",
    "@angular/platform-browser": "20.0.6",
    "@angular/platform-browser-dynamic": "20.0.6",
    "@angular/router": "20.0.6",
    "@angular/service-worker": "20.0.6",
    "@ngrx/store": "latest",
    "@ngrx/store-devtools": "latest",
    "rxjs": "latest",
    "tslib": "latest",
    "zone.js": "latest"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "latest",
    "@angular/cli": "latest",
    "@angular/compiler-cli": "20.0.6",
    "@types/jasmine": "latest",
    "husky": "latest",
    "jasmine-core": "5.8.0",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-coverage": "latest",
    "karma-jasmine": "latest",
    "karma-jasmine-html-reporter": "latest",
    "typescript": "5.8.3",
    "webpack-bundle-analyzer": "latest"
  }
}
